<script>
window.addEventListener('DOMContentLoaded', function(){
  let hcBtn = document.querySelector("#hc-button-menu");
  let hcNav = document.querySelector("#NavDrawer");
  let root = document.getElementsByTagName( 'html' )[0];
  let mainContent = document.querySelector("#MainContent");
  
  hcNav.style.top = "93px";
  window.onscroll = function() {
    var y = window.scrollY;
    if(y == 0){
      hcNav.style.top = "93px";
    } else {
      hcNav.style.top = "58px";
    }
  };
  
  hcBtn.addEventListener("click", function() {
      hcBtn.classList.toggle("open");
  });
  window.addEventListener('click', function(e){
    if (!hcNav.contains(e.target)){
    hcBtn.classList.remove("open");
  }
})
});
</script>  
