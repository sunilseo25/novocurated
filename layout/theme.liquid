<!-- HC - Custom collection page sorting popup - 25 Nov '22 -->
{% if template contains 'collection' %}
{{ 'jquery-3.2.1.min.js' | asset_url | script_tag }}
<script>
  var $ = $.noConflict(true);
  $(document).on('click', '.hc-popup-label', function() {
    $(this).toggleClass('hc-clicked');
    $('.hc-popup').toggleClass('hc-opened');
  });
  
  $(document).on('click', '.hc-sort-option', function() {
    var hc_value = $(this).attr('value');

    $('.hc-sort-option').removeClass('hc-selected');
    $(this).addClass('hc-selected');
    $("#SortBy").val(hc_value).trigger('change');
    document.querySelector('#SortBy').dispatchEvent(new Event('change', { 'bubbles': true }));
  });
  
  $(document).on('click touch', function(event) {
    if (!$(event.target).parents().addBack().is('.hc-popup') && !$(event.target).parents().addBack().is('.hc-popup-label')) {
      $('.hc-popup').removeClass('hc-opened');
      $('.hc-popup-label').removeClass('hc-clicked');
    }
  });
</script>
{% endif %}
  
